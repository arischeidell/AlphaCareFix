<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.PatientListController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <TableView fx:id="patientTableView" layoutX="32.0" layoutY="89.0" onMouseClicked="#onBillTableViewMouseClick" onSort="#onPatientTableSort" prefHeight="271.0" prefWidth="567.0">
              <columns>
                <TableColumn fx:id="firstNameColumn" prefWidth="140.0" text="First Name" />
                <TableColumn fx:id="lastNameColumn" prefWidth="150.0" text="Last Name" />
                  <TableColumn fx:id="ageColumn" prefWidth="104.0" text="Age" />
                  <TableColumn fx:id="heightColumn" prefWidth="99.0" text="Height" />
                  <TableColumn fx:id="weightColumn" prefWidth="73.0" text="Weight" />
              </columns>
            </TableView>
            <Label layoutX="242.0" layoutY="10.0" prefHeight="43.0" prefWidth="147.0" text="All Patients">
               <font>
                  <Font size="29.0" />
               </font>
            </Label>
            <Button fx:id="homeButton" layoutX="32.0" layoutY="19.0" mnemonicParsing="false" onAction="#onHomeButtonAction" text="Home" />
            <Button layoutX="534.0" layoutY="360.0" mnemonicParsing="false" onAction="#onAddUserButtonAction" text="Add User" />
         </children>
    </AnchorPane>
  </children>
</VBox>
