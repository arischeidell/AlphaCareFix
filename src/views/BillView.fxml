<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.181" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.BillController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <TableView fx:id="billTableView" layoutX="32.0" layoutY="89.0" onMouseClicked="#onBillTableViewMouseClick" prefHeight="271.0" prefWidth="567.0">
              <columns>
                <TableColumn fx:id="dateTableColumn" prefWidth="75.0" text="Date" />
                <TableColumn fx:id="itemTableColumn" prefWidth="234.0" text="Item" />
                  <TableColumn fx:id="costTableColumn" prefWidth="85.0" text="Total Cost" />
                  <TableColumn fx:id="insuranceTableColumn" prefWidth="99.0" text="Insurance Cover" />
                  <TableColumn fx:id="dueTableColumn" prefWidth="73.0" text="Due" />
              </columns>
            </TableView>
            <Label layoutX="222.0" layoutY="10.0" prefHeight="43.0" prefWidth="196.0" text="Bill Information">
               <font>
                  <Font size="29.0" />
               </font>
            </Label>
            <Button fx:id="homeButton" layoutX="32.0" layoutY="19.0" mnemonicParsing="false" onAction="#onHomeButtonAction" text="Home" />
         </children>
    </AnchorPane>
  </children>
</VBox>
